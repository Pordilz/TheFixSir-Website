{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "thefixsir.co.za"
                }
            ],
            "destination": "https://www.thefixsir.co.za/$1",
            "permanent": true
        }
    ]
}